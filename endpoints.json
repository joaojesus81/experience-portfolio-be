{
  "GET /api": {
    "description": "serves up a json representation of all the available endpoints of the api"
  },
  "GET /api/projects": {
    "description": "serves up an array of all projects",
    "queries": "Accepts all properties of a project as a query, e.g: '/api/projects?ClientName=Network Rail Limited'.  Multiple queries can be included; they are matched with AND, not OR.",
    "exampleResponse": {
      "projects": [
        {
          "ProjectCode": 22398800,
          "StaffID": 37704,
          "TotalHrs": 100,
          "experience": null,
          "experienceID": 1
        },
        {
          "ProjectCode": 246259,
          "StaffID": 37704,
          "TotalHrs": 200,
          "experience": null,
          "experienceID": 2
        }
      ]
    }
  },
